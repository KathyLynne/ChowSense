<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent" android:background="#FFFFFFFF">

    <ImageView
            android:layout_width="fill_parent"
            android:layout_height="250dp"
            android:id="@+id/imageView" android:layout_gravity="center_horizontal" android:src="@drawable/chef"
            android:paddingTop="10dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/about_us"
            android:id="@+id/textView10" android:textAlignment="center"
            android:padding="10dp" android:gravity="center_horizontal" android:textColor="#fe257618"
            android:textAppearance="?android:attr/textAppearanceMedium"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/about_people"
            android:id="@+id/textView11" android:gravity="center_horizontal" android:padding="10dp"
            android:textColor="#fe257618"/>
</LinearLayout>